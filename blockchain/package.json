{
  "name": "dequity-blockchain",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run start:prod",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main --max-old-space-size=128",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "create-test-db": "cross-env EXISTING_DB=postgres TEST_DB_NAME=blockchain_test_api node dist/src/db/create-test-db.js",
    "delete-test-db": "cross-env EXISTING_DB=postgres TEST_DB_NAME=blockchain_test_api node dist/src/db/delete-test-db.js",
    "test:api:command": "cross-env POSTGRES_DB=blockchain_test_api ACCESS_SECRET=test_secret jest --config test/jest-api.json --runInBand --forceExit",
    "test:api": "npm run -s build && npm run -s create-test-db && cross-env POSTGRES_DB=blockchain_test_api npm run -s typeorm:run && npm run -s test:api:command ; npm run -s delete-test-db",
    "test:api:coverage": "jest --config test/jest-api.json --coverage --forceExit",
    "typeorm": "typeorm-ts-node-commonjs -d dist/src/db/ormconfig.js",
    "typeorm:generate": "typeorm migration:generate ./src/migrations/migration --pretty -d dist/src/db/ormconfig.js",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "amqp-connection-manager": "^4.1.9",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "coinpayments-ipn": "^1.0.0",
    "cron": "^2.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "everscale-inpage-provider": "^0.3.63",
    "everscale-standalone-client": "^2.1.23",
    "express-status-monitor": "^1.3.4",
    "jsonwebtoken": "^9.0.0",
    "nekoton-wasm": "^1.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "swagger-ui-express": "^4.6.0",
    "tslog": "^4.4.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
